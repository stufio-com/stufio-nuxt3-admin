{
  "python.analysis.extraPaths": [
    "./nameniac_fastapi/env/lib/python3.11/site-packages/",
    "./stufio-modules-activity/",
    "./stufio-framework/"
  ],
  "sqltools.connections": [
    {
      "server": "localhost",
      "port": 8123,
      "useHTTPS": false,
      "database": "nameniac",
      "username": "default",
      "enableTls": false,
      "previewLimit": 50,
      "driver": "ClickHouse",
      "name": "clickhouse",
      "group": "nameniac",
      "password": ""
    }
