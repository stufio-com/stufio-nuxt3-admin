    "version": "0.2.0",
    "configurations": [
      {
        "name": "Go Whois Debug",
        "type": "go",
        "request": "attach",
        "mode": "remote",
        "port": 4000,
        "host": "127.0.0.1",
        "cwd": "${workspaceFolder}/gowhois/",
        "remotePath": "/app/api/"
      },
      {
        "name": "Go Ainaming Debug",
        "type": "go",
        "request": "attach",
        "mode": "remote",
        "port": 4000,
        "host": "127.0.0.1",
        "cwd": "${workspaceFolder}/goainaming/",
        "remotePath": "/app/api/"
      },
      {
        "name": "Run Phython",
        "type": "python",
        "request": "attach",
        "pathMappings": [
          {
            "localRoot": "${workspaceFolder}/fastapi",
            "remoteRoot": "/app"
          }
        ],
        "justMyCode": true,
        "port": 5679,
        "host": "127.0.0.1"
      },
      {
        "name": "Run Phython Nameniac",
        "type": "python",
        "request": "attach",
        "pathMappings": [
          {
            "localRoot": "${workspaceFolder}/nameniac_fastapi",
            "remoteRoot": "/app"
          },
          {
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/srv",
          },
        ],
        "justMyCode": false,
        "port": 5680,
        "host": "127.0.0.1"
      }
    ]
}